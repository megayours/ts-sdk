{
  "name": "@megayours/sdk",
  "version": "0.1.42",
  "description": "SDK for connecting to MegaYours dapps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "blockchain",
    "sdk",
    "megayours"
  ],
  "author": "MegaYours",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,ts,d.ts}": "eslint --fix"
  },
  "peerDependencies": {
    "@chromia/ft4": "^1.0.0",
    "postchain-client": "^1.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsup": "^8.2.4",
    "typescript-eslint": "^8.2.0",
    "@chromia/ft4": "^1.0.1"
  }
}
